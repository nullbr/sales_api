[Unit]
Description=demo fastapi application
After=network.target

[Service]
User=nullbr
Group=nullbr
WorkingDirectory=/home/nullbr/sales_api/src/
Environment="PATH=/home/nullbr/sales_api/venv/bin"
EnvironmentFile=/home/nullbr/.env
ExecStart=/home/nullbr/sales_api/venv/bin/gunicorn -w 4 -k uvicorn.workers.UvicornWorker app.main:app --bind 0.0.0.0:8000

[Install]
WantedBy=multi-user.target